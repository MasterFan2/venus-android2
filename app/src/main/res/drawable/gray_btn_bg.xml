<?xml version="1.0" encoding= "UTF-8"?>
<selector android:constantSize ="true"
    xmlns:android= "http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape android:shape= "rectangle">
            <gradient android:startColor="@color/gray_500" android:centerColor="@color/gray_500" android:endColor="@color/gray_500" android:angle="270.0" />
            <!--             <stroke android:width="1.0px" android:color="@color/more_pressed_end" /> -->
            <corners android:bottomLeftRadius= "5.0dip" android:bottomRightRadius ="5.0dip" android:topLeftRadius="5.0dip" android:topRightRadius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape= "rectangle">
            <gradient android:startColor="@color/gray_btn_color" android:centerColor="@color/gray_btn_color" android:endColor="@color/gray_btn_color" android:angle="270.0" />
            <!--             <stroke android:width="1.0px" android:color="@color/more_pressed_end" /> -->
            <corners android:bottomLeftRadius= "5.0dip" android:bottomRightRadius ="5.0dip" android:topLeftRadius="5.0dip" android:topRightRadius="5.0dip" />
        </shape>
    </item>
</selector>
